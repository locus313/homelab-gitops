# Traefik Environment Variables
# Copy this file to .env and update with your values

# CloudNS DNS provider credentials for SSL certificate generation
CLOUDNS_SUB_AUTH_ID=your_sub_auth_id_here
CLOUDNS_AUTH_PASSWORD=your_auth_password_here

# Base path for Docker volumes and configuration
DOCKER_BASE_PATH=/docker

# Internal network IP address (for NAS or internal services)
NAS_IP=192.168.1.100

# Base domain for all services (e.g., example.com)
# Services will be accessible at service.example.com
TRAEFIK_BASE_DOMAIN=example.com

# Timezone for log timestamps and scheduling
TZ=America/Los_Angeles
